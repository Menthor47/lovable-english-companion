# ============================================================================
# AGSEO Environment Configuration
# ============================================================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control
# ============================================================================

# -------------------------------------------------------------------------
# Site Configuration
# -------------------------------------------------------------------------
VITE_SITE_URL=https://agseo.pro
VITE_CONTACT_EMAIL=hello@agseo.pro
VITE_PHONE_NUMBER=+1-XXX-XXX-XXXX
VITE_ADDRESS_STREET=Your Street Address
VITE_ADDRESS_CITY=Your City
VITE_ADDRESS_COUNTRY=USA
VITE_WHATSAPP_NUMBER=15550123456

# -------------------------------------------------------------------------
# Firebase Configuration (REQUIRED)
# Get these from your Firebase project settings
# -------------------------------------------------------------------------
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abc123
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# -------------------------------------------------------------------------
# Firebase Cloud Functions (for email sending)
# Set these in Firebase project settings > Cloud Functions
# -------------------------------------------------------------------------
# RESEND_API_KEY=re_xxxxxxxxxxxx  # Set in Firebase Cloud Functions env vars

# -------------------------------------------------------------------------
# Analytics & Monitoring
# -------------------------------------------------------------------------
VITE_SENTRY_DSN=
VITE_GTM_CONTAINER_ID=GTM-XXXXXXXX
VITE_GA_MEASUREMENT_ID=
VITE_AHREFS_ANALYTICS_KEY=

# -------------------------------------------------------------------------
# Rate Limiting
# -------------------------------------------------------------------------
VITE_RATE_LIMIT_MAX=8
VITE_RATE_LIMIT_WINDOW_SECONDS=600
VITE_RATE_LIMIT_FUNCTION_URL=https://us-central1-your-project.cloudfunctions.net/checkRateLimit